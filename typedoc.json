    "projectDocuments": ["./docs/**/*.md"],
    "alwaysCreateEntryPointModule": false,
    "readme": "none",
    "includeVersion": true,
    "excludePrivate": false,
    "entryPointStrategy": "expand",
    "tsconfig": "./tsconfig.esm.json"
}
  