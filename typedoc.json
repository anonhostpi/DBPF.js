{
    "name": "DBPF.js",
    "plugin": ["typedoc-plugin-markdown"],
    "out": "./wiki",
    "entryPoints": [
        "./src/boilerplate.ts",
        "./src/BufferStore.ts",
        "./src/DBPF.ts",
        "./src/EventedPromise.ts",
        "./src/LFUCache.ts",
        "./src/polyfill.Buffer.ts",
        "./src/polyfill.events.ts",
        "./src/polyfill.fs.ts",
        "./src/polyfill.ts",
        "./src/serde.ts",
        "./src/plugins/**/*.ts"
